<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorAccent">@color/white</item>
        <item name="colorPrimary">@color/white</item>
        <item name="android:textColorPrimary">@color/white</item>
    </style>

    <style name="PauseDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/PauseDialogAnimation</item>
    </style>
    <style name="roundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">12dp</item>
        <item name="color">#00FFFFFF</item>
    </style>
    <style name="PauseDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/enter_back_to_front</item>
        <item name="android:windowExitAnimation">@anim/exit_front_to_back</item>
    </style>
    <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@drawable/text_input_box_stroke</item>
        <item name="hintTextColor">@color/black</item>
        <item name="color">#000000</item>
        <item name="boxStrokeWidth">2dp</item>
    </style>

</resources>